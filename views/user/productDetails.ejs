			<script>
				document.getElementById('addToCartForm').addEventListener('submit', function(event) {
					event.preventDefault(); // Prevent the default form submission
					
					const form = event.target;
					const formData = new FormData(form); // Gather form data
					
					formData.append('qty', '1');
					fetch(form.action, {
						method: 'POST',
						body: formData
					})
					.then(response => {
						if (response.ok) {
							// If response is successful, redirect to '/cart'
							window.location.href = '/cart';
						} else {
							// If there's an error response, handle the status message
							return response.json();
						}
					})
					.then(data => {
						// Display the error message if 'message' field exists
						if (data && data.message) {
							showMessage(data.message); // Show the error message
						}
					})
					.catch(error => {
						console.error('Error:', error);
						// Handle other errors if needed
					});
				});
			
				function showMessage(message) {
					const messageElement = document.createElement('div');
					messageElement.textContent = message;
					messageElement.classList.add('added-message');
					document.body.appendChild(messageElement);
			
					// Hide the message after 5 seconds
					setTimeout(() => {
						messageElement.style.display = 'none';
					}, 5000); // Adjusted time to show the message for 5 seconds
				}
			</script>
			<style>
				.added-message {
					position: fixed;
					top: 50px; /* Adjust vertical position */
					right: 20px; /* Adjust horizontal position */
					background-color: #fa6f05; /* Change to orange color */
					color: white; /* Text color */
					padding: 10px;
					border-radius: 5px;
					z-index: 9999;
				}
			</style>
	
